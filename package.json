{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "exports": "./src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch . --exec 'node --loader ts-node/esm -r tsconfig-paths/register' src/index.ts",
    "lint": "eslint ./ --ext ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf ./node_modules ./dist",
    "migration:generate": "npx tsx ./node_modules/drizzle-kit/bin.cjs generate",
    "migration:run": "npx tsx ./node_modules/drizzle-kit/bin.cjs migrate",
    "migration:delete": "npx tsx ./node_modules/drizzle-kit/bin.cjs drop",
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "date-fns": "2.30.0",
    "dotenv": "16.0.3",
    "dotenv-expand": "10.0.0",
    "drizzle-kit": "0.24.2",
    "drizzle-orm": "0.33.0",
    "drizzle-zod": "0.5.1",
    "helmet": "6.1.5",
    "jsonwebtoken": "9.0.0",
    "mysql2": "3.6.1",
    "tsc-alias": "1.8.8",
    "uuid": "9.0.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.14",
    "@types/express": "4.17.18",
    "@types/jsonwebtoken": "9.0.3",
    "@types/multer": "1.4.12",
    "@types/node": "20.7.0",
    "@types/node-cron": "3.0.11",
    "@types/swagger-ui-express": "4.1.4",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "prettier-eslint-cli": "7.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  }
}
